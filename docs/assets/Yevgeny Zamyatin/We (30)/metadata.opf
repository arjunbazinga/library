        <dc:title>We</dc:title>
        <dc:creator opf:file-as="Zamyatin, Yevgeny &amp; Brown, Clarence" opf:role="aut">Yevgeny Zamyatin</dc:creator>
        <dc:creator opf:file-as="Zamyatin, Yevgeny &amp; Brown, Clarence" opf:role="aut">Clarence Brown</dc:creator>
