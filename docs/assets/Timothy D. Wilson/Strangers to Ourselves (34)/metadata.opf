        <dc:title>Strangers to Ourselves</dc:title>
        <dc:creator opf:file-as="Wilson, Timothy D." opf:role="aut">Timothy D. Wilson</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (5.31.1) [https://calibre-ebook.com]</dc:contributor>
